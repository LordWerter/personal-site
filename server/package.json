{
  "name": "server",
  "private": true,
  "sideEffects": false,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --topological-dev run build",
    "clean": "yarn workspaces foreach run clean",
    "coverage": "yarn workspaces --topological-dev foreach run coverage",
    "format": "prettier --write .",
    "format-check": "prettier .",
    "lint": "eslint . --ext .js,.ts,.tsx,json",
    "lint-fix": "eslint . --ext .js,.ts,tsx,json --fix",
    "package:doctor": "yarn dlx @yarnpkg/doctor .",
    "test-ci": "yarn workspaces foreach -v run test-ci"
  },
  "devDependencies": {
    "@fast-js/eslint": "^1.2.6",
    "@fast-js/jest": "^1.0.1",
    "@fast-js/prettier": "^3.2.1",
    "@fast-js/types": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "csstype": "^3.1.0",
    "eslint": "^8.19.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-yaml": "^0.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-monorepo": "^0.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "react": "^18.2.0"
  }
}